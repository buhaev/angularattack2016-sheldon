  "scripts": {
    "test": "NODE_ENV=production babel src/components --out-dir lib && ./node_modules/.bin/babel-node ./test/index.js | tap-spec",
    "start": "NODE_ENV=development node server.js",
    "build": "rimraf build && NODE_ENV=production ./node_modules/.bin/webpack",
    "travis": "npm run coveralls",
    "prepublish": "npm run build",
    "coverage": "NODE_ENV=production babel src/components --out-dir lib && ./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover --dir cov ./test/index.js",
